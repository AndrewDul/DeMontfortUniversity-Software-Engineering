package primitives;

public class DaysOfWeek {

	int answ = 1 ;
	
	switch (answ) {
	case 1: {
		
		System.out.print("Monday");
	}case 2: {
		
		System.out.print("Monday");
	}case 3: {
		
		System.out.print("Monday");
	}case 4: {
		
		System.out.print("Monday");
	}case 5: {
		
		System.out.print("Monday");
	}case 6: {
		
		System.out.print("Monday");
	}case 7: {
		
		System.out.print("Monday");
	}
	
	
	default:
		System.out.print("Type correct data");
	}
}
